{
    "name": "robsonsanches/stays-php-client",
    "type": "library",
    "description": "PHP client for server-side integration with Stays API v1 (unofficial library)",
    "keywords": [
        "stays",
        "api",
        "php",
        "client"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robson Sanches",
            "role": "Developer, maintainer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RobsonSanches\\Stays\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RobsonSanches\\Stays\\Client\\Tests\\": "tests/"
        }
    }
}
